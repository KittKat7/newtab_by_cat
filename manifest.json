{
  "manifest_version": 3,
  "name": "My New Tab Extension",
  "version": "1.0",
  "description": "A custom new tab page.",
  "permissions": [
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "pages/home.html"
  },
  "action": {
    "default_popup": "pages/home.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}